# The GCD algorithm in Meteor
let a -> int;
let b -> int;

func gcd (a -> int, b -> int) -> int {
    while (a != b ){
        if (b < a) a = a - b;
        else b = b - a;
    }
    return a;
}

func main() -> int {
  let x -> int;
  let y -> int;
  a = 18;
  b = 9;
  x = 2;
  y = 14;
    
  print(gcd(x,y));
  print(gcd(3,15));
  print(gcd(99, 121));
  print(gcd(a,b));
  return 0;
}
